namespace abby;

table ServerA {
	price:double;
	name:string;
}

table ServerB {
	name:string;
	age:int;
}

table ServerC {
	name:string;
	weight:int;
}

table ServerImage {
	time: double;
	height: int;
	weight: int;
    image: [ubyte];
}

union ServerType {
    ServerA,
    ServerB,
	ServerC,
	ServerImage,
}

table ServerData {
    message: ServerType;
}

root_type ServerData;